<table class="table">
    <thead>
      <tr>
        <th scope="col">Username</th>
        <th scope="col">Email</th>
      </tr>
    </thead>
    <tbody>
        <% users.forEach(user => { %>
            <tr>
                <td><%= user.userName %></td>
                <td><%= user.email %></td>
              </tr>
        <% }); %>
    </tbody>
  </table>